
// Directives/preprocess: Include config and lib files.
Include("Utils.sctest");

Kustom("cheeseburger")

public class Variation999 : ScriptBase
{
    Utils u = new();

    // Required overrides.
    public override int Setup()
    {
        worldX = 100;
        worldY = 50;

        CreatePlayer(Role.Oligarch, "Vladimir");
        CreatePlayer(Role.Sycophant, "Donald");
        CreatePlayer(Role.Rebel, "Volodymyr ");
        CreatePlayer(Role.Peon, "Bob");

        bool b = u.Boing(0);
        //bool b = u.Boing(60);

        return 0;
    }    

    public override int Move()
    {
        var role = RandomRole();
        if (players.ContainsKey(role))
        {
            Print($"Role be gone:{role}");
            players.Remove(role);
        }
        else
        {
            Print($"Role already gone:{role}");
        }

        return 0;
    }
}
    